{
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/py/(.*)",
            "dest": "/api/$1"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}